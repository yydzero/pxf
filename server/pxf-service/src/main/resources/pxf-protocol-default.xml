<!--
    PXF pre-packaged protocol definition file.
    It's not recommended to add new protocols here.
    For adding new custom profiles please edit pxf-protocol.xml
-->
<profiles>
    <profile>
        <protocol>demo</protocol>
        <description>
            Protocol that showcases the functionality of PXF. The demo protocol
            does not access any data, it generates artificial splits and tuples,
            and returns the data in CSV format.
        </description>
        <plugins>
            <processor>org.greenplum.pxf.api.examples.DemoProcessor</processor>
        </plugins>
    </profile>
</profiles>
